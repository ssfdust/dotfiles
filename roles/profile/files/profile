export $(/usr/lib/systemd/user-environment-generators/30-systemd-environment-d-generator)
LF_ICONS=$(sed $HOME/.config/lf/diricons \
            -e '/^[ \t]*#/d'       \
            -e '/^[ \t]*$/d'       \
            -e 's/[ \t]\+/=/g'     \
            -e 's/$/ /')
LF_ICONS=${LF_ICONS//$'\n'/:}
export LF_ICONS
